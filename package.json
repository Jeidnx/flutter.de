{
    "name": "flutter.de",
    "version": "0.0.1",
    "scripts": {
        "start": "hugo server -D",
        "build:local": "hugo --baseURL=http://localhost:8080",
        "build:prod": "hugo --baseURL=http://flutter.de.s3-website.eu-central-1.amazonaws.com",
        "serve": "http-server public",
        "deploy:s3": "aws s3 cp public s3://flutter.de/ --recursive --acl public-read"
    },
    "private": true,
    "hugo-bin": {
        "buildTags": "extended"
    },
    "devDependencies": {
        "hugo-bin": "^0.43.6"
    }
}
